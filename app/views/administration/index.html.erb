

<br>
<div class="row">
  <div class="col-md-3"></div>
  <div class="col-md-6">
    <table class="table">
      <caption style="text-align:center;">Lista de Request</caption>

      <thead style="background-color: #54bfde;" >
        <tr>
          <th scope="row">Correo</th>
          <th>Api Key</th>
          <th>Num Request</th>
        </tr>
      </thead>

      <tbody>
        <% @items.each do |item| %>
          <tr>
            <td><%= item.correo %></td>
            <td><%= item.ApiKey %></td>
            <td><%= item.num %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <div class="col-md-3">
<a class="btn btn-info"  href="/administration/new">Registrar Nuevo Correo</a>
  </div>

</div>

