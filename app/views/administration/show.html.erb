<% if @items=="Null" %>

<div class="row">

<div class="col-md-6 col-md-offset-3">
	<div class="alert alert-danger">
	  <strong>Error!</strong> El ApiKey buscado no fue encontrado.
	</div>
</div>

<% else %>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
	<table class="table">
      <caption style="text-align:center;">Palabras Duplicadas</caption>
      <thead style="background-color: #54bfde;" >
        <tr>
          <th>Word</th>
          <th>Positions</th>
        </tr>
      </thead>

      <tbody>
        <% @vector_palabras .each do |item| %>
          <tr>
            <td><%= item[0] %></td>
          
            <td><%= item[1] %></td>
          </tr>
        <% end %>
      </tbody>
  </table>
</div>
</div>
<% end %>




	


